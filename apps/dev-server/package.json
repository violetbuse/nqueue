{
    "name": "dev-server",
    "version": "0.0.0",
    "private": true,
    "main": "./dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "node build.js",
        "build:watch": "node build.js --watch",
        "dev": "concurrently -n \"build,nodemon\" \"npm run build:watch\" \"nodemon\"",
        "start": "node dist/index.js",
        "check-types": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "concurrently": "^8.2.0",
        "esbuild": "^0.19.0",
        "tsx": "^4.0.0",
        "typescript": "^5.0.0",
        "nodemon": "^3.0.0",
        "server": "*"
    },
    "dependencies": {}
}